{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tissueanalyticsapiconnector"
		},
		"healingpartnersreporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'healingpartnersreporting'"
		},
		"itreatmedisoftcsv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'itreatmedisoftcsv'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tissueanalyticsapiconnector_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"healingpartnersreporting_connectionString": {
						"value": "[parameters('healingpartnersreporting_connectionString')]"
					},
					"itreatmedisoftcsv_connectionString": {
						"value": "[parameters('itreatmedisoftcsv_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "tissueanalyticsapiconnector_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'tissueanalyticsapiconnector_ArmTemplate_0')]"
			]
		}
	]
}