{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tissueanalyticsapiconnector"
		},
		"healingpartnersreporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'healingpartnersreporting'"
		},
		"tissueanalyticsapi_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'tissueanalyticsapi'"
		},
		"itreatmedisoftcsv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'itreatmedisoftcsv'"
		},
		"tissueanalyticsapi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://healingpartners.tissue-analytics.io/coreapi"
		},
		"tissueanalyticsapi_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "northlake_analytics"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tissueanalyticsapiconnector_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"healingpartnersreporting_connectionString": {
						"value": "[parameters('healingpartnersreporting_connectionString')]"
					},
					"tissueanalyticsapi_password": {
						"value": "[parameters('tissueanalyticsapi_password')]"
					},
					"itreatmedisoftcsv_connectionString": {
						"value": "[parameters('itreatmedisoftcsv_connectionString')]"
					},
					"tissueanalyticsapi_properties_typeProperties_url": {
						"value": "[parameters('tissueanalyticsapi_properties_typeProperties_url')]"
					},
					"tissueanalyticsapi_properties_typeProperties_userName": {
						"value": "[parameters('tissueanalyticsapi_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}