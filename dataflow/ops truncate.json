{
	"name": "ops truncate",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable16",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable16",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          patientname as string,",
				"          clinician as string,",
				"          facility as string,",
				"          ICD10 as string,",
				"          billed as string,",
				"          coded as string,",
				"          visitdate as string,",
				"          encounterid as string,",
				"          billingcodes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 alterRow(deleteIf(true())) ~> alterRow1",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          patientname as string,",
				"          clinician as string,",
				"          facility as string,",
				"          ICD10 as string,",
				"          billed as string,",
				"          coded as string,",
				"          visitdate as string,",
				"          encounterid as string,",
				"          billingcodes as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          patientname,",
				"          clinician,",
				"          facility,",
				"          ICD10,",
				"          billed,",
				"          coded,",
				"          visitdate,",
				"          encounterid,",
				"          billingcodes",
				"     )) ~> sink1"
			]
		}
	}
}